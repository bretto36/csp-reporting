{
  "name": "bretto36/csp-reporting",
  "description": "CSP Reporting for Laravel Applications",
  "type": "library",
  "require": {
    "php": "^8.1",
    "illuminate/routing": "^9|^10|^11",
    "illuminate/support": "^9|^10|^11"
  },
  "require-dev": {
    "orchestra/testbench": "^6|^7|^8|^9",
    "phpunit/phpunit": "^9.6|^10.5"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Bretto36\\CspReporting\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bretto36\\CspReporting\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Bretto36\\CspReporting\\ServiceProvider"
      ],
      "aliases": {}
    }
  }
}
